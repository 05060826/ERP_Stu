<script>
    $(function () {
        var id =  @ViewBag.id;
        if (id != -1) {

            fan();
        }
       


    })


    function Xiao() {

        $.ajax({
            url: "https://localhost:44300/api/showXiao",
            type: "get",
            dataType: "json",
            contentType: "application/json",
            success: function (result) {
                $("#xiao").empty();
                var str = "";
                str += "<option value='-1'>销售人员</option>"
                $.each(result, function (i, n) {
                    str += "<option value='" + n.seId + "' >" + n.seName + "</option>"
                })
                $("#xiao").append(str);

                Show('First');
            }
        })





</script>

